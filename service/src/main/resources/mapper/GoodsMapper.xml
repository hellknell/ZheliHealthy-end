                        when id = #{item.id,jdbcType=INTEGER} then #{item.tag,jdbcType=LONGVARCHAR}
                    </if>
                </foreach>
            </trim>
        </set>
        where id in
        <foreach close=")" collection="list" item="item" open="(" separator=", ">
            #{item.id,jdbcType=INTEGER}
        </foreach>
